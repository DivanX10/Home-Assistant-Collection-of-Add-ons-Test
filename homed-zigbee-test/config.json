{
  "name": "HOMEd ZigBee Test",
  "version": "3.4.0",
  "slug": "homedzigbee",
  "description": "HOMEd ZigBee тестовый вариант",
  "uart": true,
  "url": "https://wiki.homed.dev/page/HOMEd",
  "startup": "application",
  "arch": [
    "amd64",
    "arm64",
    "armv7"
  ],
  "boot": "auto",
  "map": [
    "share:rw",
    "config:rw"
  ],
  "ports": {
    "8080/tcp": 8080
  },
  "options": {
    "data_path": "/config/homed",
    "mqtt": {},
    "serial": {}
  },
  "schema": {
    "data_path": "str"
  },
  "mqtt": {
    "server": "str?",
    "ca": "str?",
    "key": "str?",
    "cert": "str?",
    "user": "str?",
    "password": "str?"
  }
}
