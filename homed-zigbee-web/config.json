{
  "name": "HOMEd ZigBee WEB",
  "version": "0.0.1",
  "url": "https://wiki.homed.dev/page/HOMEd",
  "slug": "homed_zigbee_web",
  "description": "HOMEd ZigBee + WEB альтернативный вариант Zigbee2MQTT",
  "webui": "http://[HOST]:[PORT:8080]",
  "arch": ["amd64"],
  "startup": "application",
